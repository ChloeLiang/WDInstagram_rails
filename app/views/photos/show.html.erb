<%= image_tag @photo.photo_url, style:'height: 300px;' %>
<p>
  <%= @photo.caption %>
</p>

<p>
  <strong>Author:</strong>
  <%= @photo.author %>
</p>

<p>
  <strong>Date taken:</strong>
  <%= @photo.date_taken %>
</p>

<%= link_to 'Edit', edit_photo_path %>
<%= link_to 'Delete', photo_path,
    method: :delete,
    data: {confirm: 'Are you sure?'} %>
<%= link_to 'Back', photos_path %>
